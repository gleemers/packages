name => tz
version => 0.1.0
description => A blazingly fast compression utility written in Rust
author => Thoq
license => MIT
deps => :

unix_commands => rm -rf tz
          > git clone https://github.com/thoq-jar/tz.git
          > cd tz && cargo build --release
          > cd tz && mv target/release/tz $HOME/.local/bin/tz
          > rm -rf tz

unix_uninstall => rm -rf $HOME/.local/bin/tz
